#!groovy
//Одновременно запустится только 1 билд
properties([disableConcurrentBuilds()])

pipeline {
    agent {
        label 'master'
        }
        options{
            buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
            timestamps()
        }
    stages {
            stage("nalichie") {
                    steps {
                            sh 'ls -la /home/maks'
                    }
            }
            stage("perehod") {
                    steps {
                            sh 'cd /home/maks/app'
                    }
            }         
            }
    }    
} 
