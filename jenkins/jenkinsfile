#!groovy
//Одновременно запустится только 1 билд
properties([disableConcurrentBuilds])

pipeline {
    agent {
        label 'master'
        }
        options{
            buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
            timestamps()
        }
    stages {
            stage("Preparations") {
                    steps {
                            sh 'ls -la /home/maks'
                    }
            }
    }    
} 